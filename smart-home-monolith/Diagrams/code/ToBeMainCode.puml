@startuml
title Умный Дом Code Diagram

top to bottom direction

!includeurl https://raw.githubusercontent.com/RicardoNiepel/C4-PlantUML/master/C4_Component.puml

class User {
  +String name
  +String email
  +List<House> houses
  +List<IoTDevice> iotdevices
  +void register()
  +void login()
}

class House {
  +String adress
  +String house_ID
  +List<IoTDevice> iotdevices
  +void editdevicelist()
  +void addhouse()
  +void removehouse()
  +void edithouse()
}

class IoTDevice {
  +String Device_ID
  +String type_ID
  +String house_ID
  +String serial_number
  +Boolean status
  +List<Module> modules
  +List<TelemetryData> data
  +void adddevice()
  +void removedevice()
}

class Devicetype {
  +String type_ID
  +String BasicInformation
}

Class TelemetryData{
  +Date date
  +String Device_ID
  +String incident_type
  +String incident_info
  +String incident_id
  +void writelog()
  +void sendlog()
}

Class Module {
  +String Module_ID
  +String Device_ID
  +Boolean status
}

User "1" -- "0..*" House : has
User "1" -- "0..*" IoTDevice : has
House "1" -- "0..*" IoTDevice : includes
IoTDevice "1" -- "0..*" Module : includes
IoTDevice "1" -- "1" Devicetype : has
IoTDevice "1" -- "0..*" TelemetryData : includes

@enduml